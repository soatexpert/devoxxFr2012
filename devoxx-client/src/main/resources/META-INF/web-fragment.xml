<?xml version="1.0" encoding="UTF-8"?>
<web-fragment xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd"
	version="3.0">
	
	<servlet>
		<description></description>
		<display-name>GiveResponse</display-name>
		<servlet-name>GiveResponse</servlet-name>
		<servlet-class>fr.soat.devoxx.game.servlets.ResponseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GiveResponse</servlet-name>
		<url-pattern>/GiveResponse</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description></description>
		<display-name>AllQuestions</display-name>
		<servlet-name>AllQuestions</servlet-name>
		<servlet-class>fr.soat.devoxx.game.servlets.QuestionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AllQuestions</servlet-name>
		<url-pattern>/AllQuestions</url-pattern>
	</servlet-mapping>
	
</web-fragment>